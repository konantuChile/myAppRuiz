<p id="notice"><%= notice %></p>

<h1>Listing Empresas</h1>

<table>
  <thead>
    <tr>
      <th>Idtamanoempresa</th>
      <th>Nombreempresa</th>
      <th>Direccionempresa</th>
      <th>Comunaempresa</th>
      <th>Telefonoempresa</th>
      <th>Emailempresa</th>
      <th>Nombrecontacto</th>
      <th>Fechaingresoempresa</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @empresas.each do |empresa| %>
      <tr>
        <td><%= empresa.idtamanoempresa %></td>
        <td><%= empresa.nombreempresa %></td>
        <td><%= empresa.direccionempresa %></td>
        <td><%= empresa.comunaempresa %></td>
        <td><%= empresa.telefonoempresa %></td>
        <td><%= empresa.emailempresa %></td>
        <td><%= empresa.nombrecontacto %></td>
        <td><%= empresa.fechaingresoempresa %></td>
        <td><%= link_to 'Show', empresa %></td>
        <td><%= link_to 'Edit', edit_empresa_path(empresa) %></td>
        <td><%= link_to 'Destroy', empresa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Empresa', new_empresa_path %>
