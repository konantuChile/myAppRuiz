<p id="notice"><%= notice %></p>

<h1>Listing Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Idestadocliente</th>
      <th>Idtipomoneda</th>
      <th>Rutempresa</th>
      <th>Lineacredito</th>
      <th>Fechaingresocliente</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.idestadocliente %></td>
        <td><%= cliente.idtipomoneda %></td>
        <td><%= cliente.rutempresa %></td>
        <td><%= cliente.lineacredito %></td>
        <td><%= cliente.fechaingresocliente %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
